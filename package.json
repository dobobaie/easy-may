{
  "name": "service-easy-may",
  "version": "1.0.0",
  "env": {
    "dist_main_path": "dist-server/main.js"
  },
  "scripts": {
    "init": "node initialization/entrypoint.js",
    "routes": "node ./node_modules/tsoa/dist/cli.js routes",
    "swagger": "node ./node_modules/tsoa/dist/cli.js swagger",
    "start": "node $npm_package_env_dist_main_path",
    "dev": "concurrently \"yarn serve\" \"yarn watch\"",
    "compile": "npm run routes && npm run build",
    "build": "tsc -p ./server",
    "watch": "yarn build -- --watch",
    "serve": "nodemon $npm_package_env_dist_main_path",
    "lint": "eslint --ext .js,.ts --ignore-path .gitignore . --fix",
    "pretty": "pretty-quick --staged",
    "set:env": "env-cmd -f .env",
    "git:reset": "git fetch --all && git reset --hard --recurse-submodules && git checkout master && git pull --rebase && git submodule update --rebase --remote --recursive",
    "lint-staged": "lint-staged",
    "test": "ava",
    "test:unit": "ava ./test/specs/**/*",
    "test:e2e": "ava ./test/e2e/**/*",
    "test:jest:e2e": "jest ./test/e2e/*.spec.ts ./test/e2e/**/*.spec.ts --maxWorkers=1"
  },
  "lint-staged": {
    "*.{ts,js}": "npm run lint"
  },
  "dependencies": {
    "@hapi/boom": "^9.1.0",
    "axios": "^0.21.0",
    "bluebird": "^3.7.2",
    "cors": "^2.8.5",
    "cycle": "^1.0.3",
    "express": "^4.17.1",
    "express-winston": "^4.0.5",
    "i": "^0.3.6",
    "imagemin": "^7.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-pngquant": "^9.0.1",
    "knex": "^0.21.0",
    "ms": "^2.1.2",
    "multer": "^1.4.2",
    "pg": "^8.0.3",
    "shortid": "^2.2.15",
    "snake-case": "^3.0.3",
    "tsoa": "^2.5.14",
    "typeorm": "^0.2.24",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.0",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/amqplib": "^0.5.13",
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.32",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/imagemin": "^7.0.0",
    "@types/imagemin-gifsicle": "^7.0.0",
    "@types/imagemin-jpegtran": "^5.0.0",
    "@types/jest": "^26.0.10",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/morgan": "^1.9.1",
    "@types/ms": "^0.7.31",
    "@types/multer": "^1.4.5",
    "@types/node": "^13.13.4",
    "@types/node-fetch": "^2.5.7",
    "@types/shortid": "0.0.29",
    "@types/supertest": "^2.0.10",
    "ava": "^3.0.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "browser-env": "^3.2.5",
    "commitlint": "^11.0.0",
    "concurrently": "^5.3.0",
    "env-cmd": "^10.1.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.0.0",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "pretty-quick": "^3.0.2",
    "require-extension-hooks": "^0.3.3",
    "require-extension-hooks-babel": "^1.0.0",
    "require-extension-hooks-vue": "^2.0.0",
    "stylelint": "^10.1.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.3.0",
    "typescript": "^3.8.3",
    "vue-template-compiler": "^2.6.12"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s pretty",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
